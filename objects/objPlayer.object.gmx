            <string>under = position_meeting(x,y,objWalkable);

if(!under) {
    speed = 0;
    instance_change(objDeadPlayer,true);
}

left = position_meeting(x + lengthdir_x(BLOCK_SIZE, direction+90),
                     y + lengthdir_y(BLOCK_SIZE, direction+90), objWalkable);
right = position_meeting(x + lengthdir_x(BLOCK_SIZE, direction-90),
                            y + lengthdir_y(BLOCK_SIZE, direction-90), objWalkable);

forward = position_meeting(x + lengthdir_x(BLOCK_SIZE, direction),
                     y + lengthdir_y(BLOCK_SIZE, direction), objWalkable);

if(left &amp;&amp; !right &amp;&amp; !forward) {
    direction += 90;
    speed = PLAYER_SPEED;
    global.moving = true;
} else if(right &amp;&amp; !left &amp;&amp; !forward) {
    direction -= 90;   
    speed = PLAYER_SPEED;
    global.moving = true;
} else if(forward &amp;&amp; !left &amp;&amp; !right) {
    speed = PLAYER_SPEED;
    global.moving = true;
} else {
    speed = 0;
    global.moving = false;
}



alarm[0] = BLOCK_SIZE/PLAYER_SPEED;
</string>
